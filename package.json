{
  "name": "htmlx-parser-svast",
  "version": "0.1.0",
  "description": "Htmlx parser which generates unist/svast AST",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "test:filter": "uvu -r ts-node/register test",
    "test:generate": "node -r ts-node/register ./test/compat-samples/generate.ts",
    "test": "uvu -r ts-node/register test test.ts$",
    "prepublish": "npm run build"
  },
  "files": [
    "package.json",
    "README.md",
    "CHANGES.md",
    "LICENSE",
    "types/*",
    "dist/*"
  ],
  "keywords": [
    "svast",
    "parser",
    "htmlx"
  ],
  "author": "David Pershouse",
  "license": "MIT",
  "peerDependencies": {
    "svast": "^0.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/node": "^14.14.7",
    "chevrotain": "^7.0.3",
    "rollup": "^2.33.1",
    "rollup-plugin-dts": "^1.4.13",
    "rollup-plugin-terser": "^7.0.2",
    "svast": "^0.0.3",
    "ts-node": "^9.0.0",
    "tslib": "2.0.3",
    "typescript": "^4.0.5",
    "uvu": "^0.4.1"
  }
}
